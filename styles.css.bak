.bg-gradient-primary {
    background-color: #000000;
    background-image: linear-gradient(180deg,#26d4bf 10%,#96f63e 100%);
    background-size: cover;
}

.bg-white {
    background-color: #000000;
    background-image: linear-gradient(90deg,#26d4bf 10%,#96f63e 100%);
    background-size: cover;
}

